<div id="<%= dom_id message %>">
  <p>
    <strong>From:</strong>
    <%= message.from %>
  </p>

  <p>
    <strong>To:</strong>
    <%= message.to %>
  </p>

  <p>
    <strong>Subject:</strong>
    <%= message.subject %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= message.body %>
  </p>

  <p>
    <strong>User:</strong>
    <%= message.user_id %>
  </p>

</div>
